<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.e_inventory">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UI.User.UserActivity"></activity>
        <activity android:name=".UI.Report.ReportActivity" />
        <activity android:name=".Adapter.AdapterCategory" />
        <activity android:name=".UI.Supplier.SupplierActivity" />
        <activity android:name=".UI.Shipped.ShippeedActivity" />
        <activity android:name=".UI.Recieved.RecievedActivity" />
        <activity android:name=".UI.Profile.ProfileActivity" />
        <activity android:name=".UI.Product.ProductActivity" />
        <activity android:name=".UI.Login.LoginActivity" />
        <activity android:name=".UI.Category.CategoryActivity" />
        <activity android:name=".UI.Beranda.BerandaActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>